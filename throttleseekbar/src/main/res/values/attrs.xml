<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ThrottleSeekbar">
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="direction" format="enum">
            <enum name="normal" value="0"/>
            <enum name="reverse" value="1"/>
        </attr>
        <attr name="offset" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="minValue" format="integer"/>
        <attr name="resetSpeed" format="integer"/>

        <attr name="startProgressColor" format="color"/>
        <attr name="middleProgressColor" format="color"/>
        <attr name="endProgressColor" format="color"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="strokeColor" format="color"/>
    </declare-styleable>
</resources>